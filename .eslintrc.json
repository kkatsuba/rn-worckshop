{
  "env": {
    "es6": true,
    "react-native/react-native": true
  },
  "plugins": [
    "react",
    "react-native",
    "json"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalDecorators": true
    }
  },
  "rules": {
    "react/display-name": 1,
    "indent": ["error", 2,
    {
      "SwitchCase": 1,
      "VariableDeclarator": {
        "var": 2,
        "let": 2,
        "const": 3
      }
    }],
    "linebreak-style": ["error", "unix"],
    "quotes": [
    1, "single", "avoid-escape"],
    "semi": ["error", "always"],
    "no-return-await": 2,
    "no-cond-assign": 2,
    "no-console": 2,
    "no-const-assign": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-dupe-keys": 2,
    "no-dupe-args": 2,
    "no-duplicate-case": 2,
    "no-else-return": 1,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-mixed-spaces-and-tabs": [
    2, false],
    "no-multiple-empty-lines": [
    1,
    {
      "max": 2
    }],
    "no-negated-in-lhs": 2,
    "no-nested-ternary": 1,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-redeclare": 1,
    "no-regex-spaces": 2,
    "no-spaced-func": 1,
    "no-sparse-arrays": 2,
    "no-trailing-spaces": [
    1,
    {
      "skipBlankLines": true
    }],
    "no-undef": 2,
    "no-unreachable": 2,
    "no-unused-vars": [
    1,
    {
      "vars": "all",
      "args": "after-used"
    }],
    "no-void": 1,
    "no-var": 2,
    "arrow-spacing": 1,
    "comma-dangle": [
    1, "only-multiline"],
    "comma-spacing": ["error",
    {
      "before": false,
      "after": true
    }],
    "complexity": [
    0, 11],
    "keyword-spacing": ["error",
    {
      "after": true
    }],
    "eqeqeq": [
    2, "allow-null"],
    "semi-spacing": [
    1,
    {
      "before": false,
      "after": true
    }],
    "object-curly-spacing": [
    2, "always"],
    "padded-blocks": [
    1, "never"],
    "prefer-const": 2,
    "space-before-blocks": [
    1, "always"],
    "space-in-parens": [
    1, "never"],
    "space-infix-ops": 1,
    "use-isnan": 2,
    "valid-typeof": 2,
    "yoda": [
    1, "never"]
  }
}
